FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      f_1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 111.111;

boundaryField
{
    boundaryWall
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 111.111;
    }
    frontAndBack
    {
        type            empty;
    }
}


