FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      f_0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    topWall
    {
        type            empty;
    }
    bottomWall
    {
        type            empty;
    }
    rightWall
    {
        type            lbmWall;
        value           uniform 0;
    }
    leftWall
    {
        type            lbmWall;
        value           uniform 0;
    }
    frontAndBack
    {
        type            empty;
    }
}


