FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      velocitySet;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// directions number
dirNum          9;

// inverse squared of lattice speed of sound
lICS2           3.;

// weights
// store MCD and numerators so that:
//    w_i = wNum_i / MCD
MCD             36.;
wNum_0          16.;
wNum_1          4.;
wNum_2          4.;
wNum_3          4.;
wNum_4          4.;
wNum_5          1.;
wNum_6          1.;
wNum_7          1.;
wNum_8          1.;

// lattice vectors
lattVect_0      ( 0.  0.  0.);
lattVect_1      ( 1.  0.  0.);
lattVect_2      ( 0.  0.  1.);
lattVect_3      (-1.  0.  0.);
lattVect_4      ( 0.  0. -1.);
lattVect_5      ( 1.  0.  1.);
lattVect_6      (-1.  0.  1.);
lattVect_7      (-1.  0. -1.);
lattVect_8      ( 1.  0. -1.);


