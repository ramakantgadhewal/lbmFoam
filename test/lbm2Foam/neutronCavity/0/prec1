FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      prec1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 0 0 0];

internalField   uniform 6.70888e+10;

boundaryField
{
    boundaryWall
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


